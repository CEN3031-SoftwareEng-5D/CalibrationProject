    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-validator": "^6.2.0",
    "formidable": "^1.2.1",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.5.1",
